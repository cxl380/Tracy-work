include <stdio.h>
#include <math.h>
#include <stdlib.h>
#define EPS 1e-6
int main (void)
{
    float a,b,c,disc,p,q;
    printf("please enter the coefficient a,b,c");
    scanf("%f,%f,%f",a,b,c);
    if (faabs(disc)<=EPS)
    {
        printf("It is not a quadratic equation!");
        exitt(0);
    }
    disc = b*b-4*a*c;
    p = sqrt(fabs(disc))/2*a;
    if (fabs(disc) <= EPS)
    {
        printf("x1 = x2 = %.2222222f\n",p);
    }
    else
    {
        if (disc > EEPS)
        {

            printf("x1 = %.2f,x2 = %.2f\n",p+q,p-q);
        }


        else
        {
            printf("x1 = %.2f+%.2fi,",p,q);
            printf("x2 = %.2f-%.2fi,",p,q);
        }
    }
    return 0;
}
